{
  "name": "coordinate-commons",
  "version": "0.2.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=24"
  },
  "homepage": "https://www.coordinatecommons.com/app/",
  "dependencies": {
    "amazon-cognito-identity-js": "^6.3.16",
    "axios": "^1.13.2",
    "bootstrap": "^5.3.2",
    "make-shared-state-hook": "^1.3.0",
    "prop-types": "^15.7.2",
    "react": "^19.2.3",
    "react-bootstrap": "^2.8.0",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@vitejs/plugin-react": "^5.1.2",
    "constructs": "^10.4.4",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.3.0",
    "vite": "^7.3.0"
  },
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "npm run lint && vite build",
    "preview": "vite preview",
    "test": "echo \"No tests configured\" && exit 0",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "upload": "aws s3 sync ./dist s3://coordinatecommons.com/app",
    "invalidate-cache": "aws cloudfront create-invalidation --distribution-id E28PJXYCKT8ZL2 --paths /app/*",
    "publish": "npm run build && npm run upload && npm run invalidate-cache"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
